{
    "name": "prestashop/core-upgrade-bundle",
    "description": "This symfony bundle provides the core files to help you upgrade your PrestaShop project to the latest version with Upgrade assistant applications.",
    "type": "symfony-bundle",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "PrestaShop SA",
            "email": "contact@prestashop.com"
        },
        {
            "name": "PrestaShop Community",
            "homepage": "https://contributors.prestashop-project.org/"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "6.4.*",
        "symfony/http-client": "6.4.*"
    },
    "autoload": {
        "psr-4": {
            "PrestaShop\\CoreUpgradeBundle\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.51",
        "phpstan/phpstan": "^1.10"
    }
}
